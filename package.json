    "cross-env": "^5.1.5",
    "debug": "~2.6.9",
    "express": "^4.16.3",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.1.1",
    "morgan": "~1.9.0",
    "nodemon": "^1.17.4"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
